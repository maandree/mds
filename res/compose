<shift compose> ("." "<") : "‚Äπ"
<shift compose> ("." ">") : "‚Ä∫"
<compose> ("x" "+") : "‚Äª"
<shift compose> "!" "!" : "‚Äº"
<compose> "!" "?" : "‚ÄΩ"
<shift compose> "^" "_" : "‚Äæ"
<shift compose> ("_" "u") : "‚Äø"
<shift compose> ("^" "u") : "‚ÅÄ"
<shift compose> "/" "/" : "‚ÅÅ"
<compose> "*" "*" : "‚ÅÇ"
<shift compose> "-" "-" : "‚ÅÉ"
<compose> "/" "/" : "‚ÅÑ"
<compose> ("[" "-") : "‚ÅÖ"
<compose> ("]" "-") : "‚ÅÜ"
<shift compose> "?" "?" : "‚Åá"
<shift compose> "?" "!" : "‚Åà"
<shift compose> "!" "?" : "‚Åâ"
<compose> ("/" "7"): "‚Åä"
<compose> "¬∂" "¬∂" : "‚Åã"
<compose> ("?" ["p" "P"]) : "‚Åã"
<compose> "(" "|" : "‚Åå"
<compose> "|" ")" : "‚Åç"
