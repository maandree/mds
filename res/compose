# Domino Tiles:
<shift compose> "d" "t" "-" ["-" <space>] : "\u1F030"
<shift compose> "d" "t" "|" ["|" <space>] : "\u1F062"
for "0" to "6" as \1
  for "0" to "6" as \2
    <shift compose> "d" "t" "-" "\1" "\2" : "\add(\u1F031 \add(\mul(\sub(\1 "0") 7) \sub(\2 "0")))"
    <shift compose> "d" "t" "|" "\1" "\2" : "\add(\u1F063 \add(\mul(\sub(\1 "0") 7) \sub(\2 "0")))"
  end for
end for

# Playing Cards:
<shift compose> "p" "c" : "üÇ†"
dead_ascii(üÇ†)
macro playing_card/2
  <compose> (["‚ô†" "‚ô§"] "\1") : "\add(\u1F0A0 \2)"
  <compose> (["‚ô•" "‚ô°"] "\1") : "\add(\u1F0B0 \2)"
  <compose> (["‚ô¶" "‚ô¢"] "\1") : "\add(\u1F0C0 \2)"
  <compose> (["‚ô£" "‚ôß"] "\1") : "\add(\u1F0D0 \2)"
end macro
for "1" to "9" as \1
  playing_card("\1" \sub(\1 "0") 1)
endfor
playing_card(["a" "A"] 1)
playing_card("0" 10)
playing_card(["j" "J"] 11)
playing_card(["c" "C"] 12)
playing_card(["q" "Q"] 13)
playing_card(["k" "K"] 14)
