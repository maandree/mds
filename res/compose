<shift compose> "-" "~" : "‚Åì"
<sjift compose> ("_" "U") : "‚Åî"
<compose> ("x" "*") : "‚Åï"
<compose> "." ":" <space>  : "‚Åñ"
<compose> ("4" ["'" "¬¥"]) : "‚Åó"
<compose> "." ":" "." : "‚Åò"
<compose> ":" "." ":" : "‚Åô"
